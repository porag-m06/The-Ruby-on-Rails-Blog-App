<section class="user-container">
<div>
  <%= render partial: 'user_profile', locals: {user: @user}%>
</div>
<div class="bio">
<h3>Bio</h3>
<p><%= @user.bio%></p>
</div>
<div class="posts">
 <%= render partial: 'user_posts', locals: {user: @user}%>
</div>
<div class="posts-details">
 <%= link_to user_posts_path(@user.id) do %>
    <button type="button" >See all posts</button>
 <% end %>
</div>

</section>